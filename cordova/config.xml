<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.kabisa.kudoomatic" version="1.0.23" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>kudo-o-matic</name>
    <team_id>9H6JXDUXPW</team_id>
    <description />
    <content src="index.html" />
    <access origin="*" />
    <hook src="../node_modules/maji/cordova/hooks/before_platform_add.js" type="before_platform_add" />
    <platform name="ios">
        <hook src="scripts/target_attributes.rb" type="after_build" />
        <hook src="scripts/share_schemes.rb" type="after_platform_add" />
    </platform>
    <icon src="../src/assets/logo/AppStore-1024.png" />
    <platform name="ios">
        <icon height="76" src="../src/assets/logo/AppStore-76.png" width="76" />
        <icon height="120" src="../src/assets/logo/AppStore-120.png" width="120" />
        <icon height="152" src="../src/assets/logo/AppStore-152.png" width="152" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="local" />
    <platform name="android">
        <config-file parent="/manifest" target="AndroidManifest.xml">
            <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
            <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
            <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
        </config-file>
    </platform>
    <engine name="android" spec="~6.2.3" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-networkactivityindicator" spec="~0.1.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.0" />
    <plugin name="cordova-plugin-googleplus" spec="git+https://github.com/EddyVerbruggen/cordova-plugin-googleplus.git">
        <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.242980876083-0cksfoq3gqljjc7n0b1mflpasg0t544l" />
    </plugin>
</widget>
